@model FloristManagement.Models.Bouquet

@{
    ViewBag.Title = "Import Excel";
    Layout = "~/Areas/Admin/Views/Shared/AdminLayoutPage.cshtml";
}
@using (Html.BeginForm("ImportExcel", "Bouquets", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    <label>
        Choose file
    </label>
    <input type="file" name="file" />
    <input type="submit" value="Import" />
}
